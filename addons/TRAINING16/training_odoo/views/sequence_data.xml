<odoo>
    <data noupdate="1">
 
 
        <!-- Sequences Training Course -->
 
        <record id="seq_sale_order" model="ir.sequence">
            <field name="name">Training Course</field>
            <field name="code">training.course</field>
            <field name="prefix">TC/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_session_order" model="ir.sequence">
            <field name="name">Training session</field>
            <field name="code">training.session</field>
            <field name="prefix">TS/%(year)s/%(month)s/</field>
            <field name="padding">3</field>
        </record>
 
 
    </data>
</odoo>